<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="partially_translated_locale" translatable="false">
        <!-- Ensure that the list of partially translated languages will not be
        present as a translated language, even if the resources are available in
        Android SystemAssetLocales. -->
        <item>gu-IN</item>
        <item>kn-IN</item>
        <item>ml-IN</item>
        <item>mr-IN</item>
        <item>pa-Guru-IN</item>
        <item>te-IN</item>
        <item>is-IS</item>
    </string-array>

    <!-- Used in LocalePicker -->
    <string-array translatable="false" name="special_locale_codes">
        <!-- http://b/17150708 - ensure that the list of languages says "Arabic"
             rather than "Arabic (Egypt)". If you're an OEM supporting multiple
             Arabic locales, you should remove this entry so that ar_EG is shown
             as "Arabic (Egypt)" in Settings. -->
        <item>ar_EG</item>

        <item>zh_CN</item>
        <item>zh_TW</item>
        <!-- IKL10NTOOLS-2743 Myanmar locale -->
        <item>my_MM</item>
        <!-- IKL10NTOOLS-2743 Myanmar locale for Zawgyi -->
        <item>en_ZG</item>
    </string-array>

    <!-- Used in LocalePicker -->
    <string-array translatable="false" name="special_locale_names">
        <!-- http://b/17150708 - ensure that the list of languages says "Arabic"
             rather than "Arabic (Egypt)". If you're an OEM supporting multiple
             Arabic locales, you should remove this entry so that ar_EG is shown
             as "Arabic (Egypt)" in Settings. -->
        <item>العربية</item>

        <item>中文 (简体)</item>
        <item>中文 (繁體)</item>
        <!-- IKL10NTOOLS-2743 Myanmar locale -->
        <item>ဗမာ (Myanmar)</item>
        <!-- IKL10NTOOLS-2743 Myanmar locale for Zawgyi -->
        <item>ဗမာ (Zawgyi)</item>
    </string-array>

</resources>