<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rxjava2_android_sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/rxjava"
        android:roundIcon="@mipmap/rxjava"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OperatorsActivity" />
        <activity android:name=".NetworkingActivity" />
        <activity android:name=".RxBindingActivity" />
        <activity android:name=".ThrottlingBackpressureActivity" />
        <activity android:name=".throttlingbackpressure.ThrottlingActivity" />
        <activity android:name=".throttlingbackpressure.BackpressureActivity"></activity>
    </application>

</manifest>