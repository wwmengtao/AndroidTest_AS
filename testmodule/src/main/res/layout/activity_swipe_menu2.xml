<?xml version="1.0" encoding="utf-8"?>
<com.example.testmodule.ui.swipemenu.SwipeMenu
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_swipemenu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ui.swipemenu.SwipeMenuActivity">

    <include layout="@layout/swipe_menu_left"
        android:id="@+id/swipe_menu_left_layout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>

    <include layout="@layout/swipe_menu_right"
        android:id="@+id/swipe_menu_right_layout"
        android:layout_alignParentEnd="true"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>

</com.example.testmodule.ui.swipemenu.SwipeMenu>
